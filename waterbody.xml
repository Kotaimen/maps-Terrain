<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over" 

>

<Style name="world_lakes" filter-mode="first">
  <Rule>
    <MinScaleDenominator>12500000</MinScaleDenominator>
	<PolygonSymbolizer fill="#668AA5" gamma="1" />
  </Rule>
</Style>
<Style name="world_rivers" filter-mode="first">
  <Rule>
    <MinScaleDenominator>12500000</MinScaleDenominator>
	<LineSymbolizer stroke-linejoin="round" stroke="#668AA5" stroke-width="0.3" />
  </Rule>
</Style>

<Style name="water" filter-mode="first">
  <Rule>
    <MaxScaleDenominator>12500000</MaxScaleDenominator>
	<PolygonSymbolizer fill="#668AA5" gamma="0.5" />
  </Rule>
</Style>

<Layer name="water_uc"
  srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">
    <StyleName>water</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[/Users/Kotaimen/proj/geodata/uc_waterbody/uc_waterbody.shp]]></Parameter>
       <Parameter name="type"><![CDATA[shape]]></Parameter>
    </Datasource>
</Layer>


</Map>

